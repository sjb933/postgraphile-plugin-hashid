{
  "extends": "tslint-config-airbnb",
  "rules": {
    "trailing-comma": false,
    "ter-arrow-parens": [true, "as-needed"],
    "quotemark": [true, "single"],
    "no-boolean-literal-compare": false,
    "arrow-return-shorthand": [true, "multiline"],
    "import-name": false,
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore"
    ]
  }
}
